{
    "name": "alexlimon404/tg-info-bugs",
    "description": "Telegram Info Bugs",
    "license": "MIT",
    "homepage": "https://tg-info.forumbase.net",
    "authors": [
        {
            "name": "AlexLimon",
            "email": "thebestprogramm@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "AlexLimon404\\TgInfoBugs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlexLimon404\\TgInfoBugs\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlexLimon404\\TgInfoBugs\\TgInfoBugsServiceProvider"
            ],
            "aliases": {
                "TgInfoBugs": "AlexLimon404\\TgInfoBugs\\Facades\\TgInfoBugs"
            }
        }
    }
}
